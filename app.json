{
  "expo": {
    "name": "native-app",
    "slug": "native-app",
    "version": "1.0.0",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "backgroundColor": "#ccbe21",
    "extra": {"eas": {"projectId": "dsaga-afdafsdf-sadfasd-afggds"}},
    "splash": {
      "image": "./assets/splash.gif",

      "resizeMode": "cover",
      "backgroundColor": "#ccbe21"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/FiraSans-Regular.tts", 
            "./assets/fonts/FiraSans-SemiBold.tts"
          ]
        }
      ],
      [
        "expo-notifications",
          {
            "icon": "./assets/favicon.png",
            "color": "#ffffff"
          }
      ]
    ],
    "scheme": "native-app"
  }
}
